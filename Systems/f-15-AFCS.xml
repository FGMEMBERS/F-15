<?xml version="1.0"?>

<!-- F-15 Autopilot -->
<!-- Retuned and Overhauled -->
<!-- (c) 2017 Joshua Davidson (it0uchpods) -->

<PropertyList>

 <!-- =============================================================== -->
 <!-- Predictors                                                      -->
 <!-- =============================================================== -->

	<predict-simple>
		<name>ALTITUDE 2 SECONDS AHEAD</name>
		<debug>false</debug>
		<input>/instrumentation/altimeter/indicated-altitude-ft</input>
		<output>/autopilot/internal/altitude-2-sec-ahead</output>
		<seconds>2.0</seconds>
		<filter-gain>0.0</filter-gain>
	</predict-simple>

 <!-- =============================================================== -->
 <!-- Autopilot Helpers                                               -->
 <!-- =============================================================== -->

	<filter>
		<name>Magnetic Heading Error Normalizer</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>/autopilot/internal/heading-bug-error-deg</input>
		<output>/autopilot/internal/heading-bug-error-deg-norm</output>
		<min>-30</min>
		<max>30</max>
	</filter>
	
	<filter>
		<name>True Heading Error Normalizer</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>/autopilot/internal/true-heading-error-deg</input>
		<output>/autopilot/internal/true-heading-error-deg-norm</output>
		<min>-30</min>
		<max>30</max>
	</filter>
	
	<logic>
		<input>
			<or>
				<equals>
					<property>/autopilot/locks/heading</property>
					<value>wing-leveler</value>
				</equals>
				<equals>
					<property>/autopilot/locks/heading</property>
					<value>dg-heading-hold</value>
				</equals>
				<equals>
					<property>/autopilot/locks/heading</property>
					<value>true-heading-hold</value>
				</equals>
			</or>
		</input>
		<output>/autopilot/internal/roll-active</output>
	</logic>
	
	<logic>
		<input>
			<or>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>pitch-hold</value>
				</equals>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>vertical-speed-hold</value>
				</equals>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>altitude-hold</value>
				</equals>
			</or>
		</input>
		<output>/autopilot/internal/pitch-active</output>
	</logic>
	
	<filter>
		<name>Aileron Normalizer</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression> <!-- Richard's Deadzone Equation -->
				<product>
					<div>
						<property>/controls/flight/aileron</property>
						<pow>
							<value>1</value>
							<property>/controls/flight/aileron</property>
						</pow>
					</div>
					<product>
						<max>
							<value>0</value>
							<difference>
								<abs>
									<property>/controls/flight/aileron</property>
								</abs>
								<property>fcs/aileron-deadzone</property>
							</difference>
						</max>
						<div>
							<value>1.0</value>
							<difference>
								<value>1.0</value>
								<property>fcs/aileron-deadzone</property>
							</difference>
						</div>
					</product>
				</product>
			</expression>
		</input>
		<output>/controls/flight/aileron-norm</output>
	</filter>
	
	<filter>
		<name>Elevator Normalizer</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression> <!-- Richard's Deadzone Equation -->
				<product>
					<div>
						<property>/controls/flight/elevator</property>
						<pow>
							<value>1</value>
							<property>/controls/flight/elevator</property>
						</pow>
					</div>
					<product>
						<max>
							<value>0</value>
							<difference>
								<abs>
									<property>/controls/flight/elevator</property>
								</abs>
								<property>/autopilot/settings/elevator-deadzone</property>
							</difference>
						</max>
						<div>
							<value>1.0</value>
							<difference>
								<value>1.0</value>
								<property>/autopilot/settings/elevator-deadzone</property>
							</difference>
						</div>
					</product>
				</product>
			</expression>
		</input>
		<output>/controls/flight/elevator-norm</output>
	</filter>

 <!-- =============================================================== -->
 <!-- Lateral Modes                                                   -->
 <!-- =============================================================== -->

	<filter>
		<name>IT-CONTROLLER: HDG KP (P Gain)</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<table>
					<property>/velocities/mach</property>
					<entry><ind>0.2</ind><dep>-1.8</dep></entry>
					<entry><ind>1.0</ind><dep>-3.7</dep></entry>
					<entry><ind>1.5</ind><dep>-3.9</dep></entry>
					<entry><ind>2.1</ind><dep>-4.1</dep></entry>
				</table>
			</expression>
		</input>
		<output>/autopilot/internal/kp-hdg</output>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: HDG LIMIT</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<table>
					<property>/velocities/mach</property>
					<entry><ind>0.2</ind><dep>30</dep></entry>
					<entry><ind>1.0</ind><dep>60</dep></entry>
				</table>
			</expression>
		</input>
		<output>/autopilot/internal/bank-limit</output>
	</filter>

	<pi-simple-controller>
		<name>HEADING MAGNETIC</name>
		<debug>false</debug>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/locks/heading</property>
					<value>dg-heading-hold</value>
				</equals>
			</condition>
		</enable>
		<input>
			<property>/autopilot/internal/heading-bug-error-deg-norm</property>
		</input>
		<reference>
			<value>0.0</value>
		</reference>
		<output>
			<property>/autopilot/internal/target-roll-deg</property>
		</output>
		<config>
			<Kp>
				<property>/autopilot/internal/kp-hdg</property>
			</Kp>
			<Ki>-0.000001</Ki>
			<min>
				<property>/autopilot/internal/bank-limit</property>
				<scale>-1</scale>
			</min>
			<max>
				<property>/autopilot/internal/bank-limit</property>
			</max>
		</config>
	</pi-simple-controller>
	
	<pi-simple-controller>
		<name>HEADING TRUE</name>
		<debug>false</debug>
		<enable>
			<condition>
				<equals>
					<property>/autopilot/locks/heading</property>
					<value>true-heading-hold</value>
				</equals>
			</condition>
		</enable>
		<input>
			<property>/autopilot/internal/true-heading-error-deg-norm</property>
		</input>
		<reference>
			<value>0.0</value>
		</reference>
		<output>
			<property>/autopilot/internal/target-roll-deg</property>
		</output>
		<config>
			<Kp>
				<property>/autopilot/internal/kp-hdg</property>
			</Kp>
			<Ki>-0.000001</Ki>
			<min>
				<property>/autopilot/internal/bank-limit</property>
				<scale>-1</scale>
			</min>
			<max>
				<property>/autopilot/internal/bank-limit</property>
			</max>
		</config>
	</pi-simple-controller>
	
	<filter>
		<name>ROLL DEG SYNC</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<and>
					<not-equals>
						<property>/autopilot/locks/heading</property>
						<value>dg-heading-hold</value>
					</not-equals>
					<not-equals>
						<property>/autopilot/locks/heading</property>
						<value>true-heading-hold</value>
					</not-equals>
				</and>
			</condition>
		</enable>
		<input>/orientation/roll-deg</input>
		<output>/autopilot/internal/target-roll-deg</output>
		<min>
			<property>/autopilot/internal/bank-limit</property>
			<scale>-1</scale>
		</min>
		<max>
			<property>/autopilot/internal/bank-limit</property>
		</max>
	</filter>

	<filter>
		<name>IT-CONTROLLER: TARGET ROLL CMD</name>
		<debug>false</debug>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>/autopilot/internal/target-roll-deg</input>
		<output>/autopilot/internal/target-roll</output>
		<type>noise-spike</type>
		<max-rate-of-change>
			<condition>
				<and>
					<or>
						<equals>
							<property>/autopilot/locks/heading</property>
							<value>dg-heading-hold</value>
						</equals>
						<equals>
							<property>/autopilot/locks/heading</property>
							<value>true-heading-hold</value>
						</equals>
					</or>
					<equals>
						<property>/controls/flight/aileron-norm</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<value>20</value>
		</max-rate-of-change>
		<max-rate-of-change>10000</max-rate-of-change>
	</filter>

	<filter>
		<name>System Command: Roll Rate</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>-1.8</gain>
		<enable>
			<condition>
				<or>
					<equals>
						<property>/autopilot/locks/heading</property>
						<value>wing-leveler</value>
					</equals>
					<equals>
						<property>/autopilot/locks/heading</property>
						<value>dg-heading-hold</value>
					</equals>
					<equals>
						<property>/autopilot/locks/heading</property>
						<value>true-heading-hold</value>
					</equals>
				</or>
			</condition>
		</enable>
		<input>
			<property>/orientation/roll-deg</property>
		</input>
		<reference>
			<property>/autopilot/internal/target-roll</property>
		</reference>
		<output>
			<property>/autopilot/internal/roll-rate</property>
		</output>   	
		<min>-8.0</min>
		<max>8.0</max>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: ROLL RATE KP (P Gain)</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<table>
					<property>/velocities/mach</property>
					<entry><ind>0.2</ind><dep>0.070</dep></entry>
					<entry><ind>1.0</ind><dep>0.005</dep></entry>
					<entry><ind>1.5</ind><dep>0.003</dep></entry>
					<entry><ind>2.1</ind><dep>0.002</dep></entry>
				</table>
			</expression>
		</input>
		<output>/autopilot/internal/kp-roll</output>
	</filter>
	
	<pid-controller>
		<name>IT-CONTROLLER: ROLL</name>
		<debug>false</debug>
		<enable>
			<condition>
				<and>
					<or>
						<equals>
							<property>/autopilot/locks/heading</property>
							<value>wing-leveler</value>
						</equals>
						<equals>
							<property>/autopilot/locks/heading</property>
							<value>dg-heading-hold</value>
						</equals>
						<equals>
							<property>/autopilot/locks/heading</property>
							<value>true-heading-hold</value>
						</equals>
					</or>
					<equals>
						<property>/controls/flight/aileron-norm</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<property>/orientation/roll-rate-degps</property>
		</input>
		<reference>
			<property>/autopilot/internal/roll-rate</property>
		</reference>
		<output>
			<property>/fdm/jsbsim/fcs/roll-trim-sas-cmd-norm</property>
		</output>
		<config>
			<Kp>
				<property>/autopilot/internal/kp-roll</property>
			</Kp>
			<beta>1</beta>
			<alpha>0.1</alpha>
			<gamma>0</gamma>
			<Ti>0.35</Ti>
			<Td>0.0001</Td>
			<u_min>-0.6</u_min>
			<u_max>0.6</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>IT-CONTROLLER: ROLL DEAD</name>
		<type>gain</type>
		<enable>
			<condition>
				<or>
					<and>
						<not-equals>
							<property>/autopilot/locks/heading</property>
							<value>wing-leveler</value>
						</not-equals>
						<not-equals>
							<property>/autopilot/locks/heading</property>
							<value>dg-heading-hold</value>
						</not-equals>
						<not-equals>
							<property>/autopilot/locks/heading</property>
							<value>true-heading-hold</value>
						</not-equals>
					</and>
					<not-equals>
						<property>/controls/flight/aileron-norm</property>
						<value>0</value>
					</not-equals>
				</or>
			</condition>
		</enable>
		<gain>1.0</gain>
		<input>0</input>
		<output>/fdm/jsbsim/fcs/roll-trim-sas-cmd-norm</output>
	</filter>

 <!-- =============================================================== -->
 <!-- Vertical Modes                                                  -->
 <!-- =============================================================== -->

	<filter>
		<name>ALTITUDE SYNC</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<and>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>altitude-hold</value>
					</equals>
					<not-equals>
						<property>/controls/flight/elevator-norm</property>
						<value>0</value>
					</not-equals>
				</and>
			</condition>
		</enable>
		<input>
			<expression>
				<product>
					<floor>
						<div>
							<sum>
								<property>/instrumentation/altimeter/indicated-altitude-ft</property>
								<value>50</value>
							</sum>
							<value>100</value>
						</div>
					</floor>
					<value>100</value>
				</product>
			</expression>
		</input>
		<output>/autopilot/settings/target-altitude-ft</output>
		<min>0</min>
		<max>100000</max>
	</filter>

	<filter>
		<name>ALTITUDE CAPTURE/HOLD</name>
		<debug>false</debug>
		<type>gain</type>
		<input>
			<expression>
				<sum>
					<property>/autopilot/internal/altitude-2-sec-ahead</property>
					<value>-5</value>
				</sum>
			</expression>
		</input>
		<reference>
			<property>/autopilot/settings/target-altitude-ft</property>
		</reference>
		<gain>-6</gain>
		<output>
			<property>/autopilot/internal/target-fpm-alt</property>
		</output>
		<min>-2000</min>
		<max>2000</max>
	</filter>

	<filter>
		<name>IT-CONTROLLER: FPM KP (P Gain)</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<table>
					<property>/velocities/mach</property>
					<entry><ind>0.2</ind><dep>0.0023</dep></entry>
					<entry><ind>1.4</ind><dep>0.0008</dep></entry>
				</table>
			</expression>
		</input>
		<output>/autopilot/internal/vs-kp</output>
	</filter>
 
	<pid-controller>
		<name>FPM HOLD</name>
		<debug>false</debug>
		<enable>
			<condition>
				<and>
					<or>
						<equals>
							<property>/autopilot/locks/altitude</property>
							<value>vertical-speed-hold</value>
						</equals>
						<equals>
							<property>/autopilot/locks/altitude</property>
							<value>altitude-hold</value>
						</equals>
					</or>
					<equals>
						<property>/controls/flight/elevator-norm</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<property>/autopilot/internal/vert-speed-fpm</property>
		</input>
		<reference>
			<condition>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>vertical-speed-hold</value>
				</equals>
			</condition>
			<property>/autopilot/settings/vertical-speed-fpm</property>
		</reference>
		<reference>
			<condition>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>altitude-hold</value>
				</equals>
			</condition>
			<property>/autopilot/internal/target-fpm-alt</property>
		</reference>
		<output>
			<property>/autopilot/internal/target-pitch-deg</property>
		</output>
		<config>
			<Kp>
				<property>/autopilot/internal/vs-kp</property>
			</Kp>
			<Ti>2.0</Ti>
			<Td>0.0001</Td>
			<u_min>
				<condition>
					<and>
						<equals>
							<property>gear/gear[1]/wow</property>
							<value>0</value>
						</equals>
						<equals>
							<property>gear/gear[2]/wow</property>
							<value>0</value>
						</equals>
					</and>
				</condition>
				<value>-15</value>
			</u_min>
			<u_min>
				<condition>
					<or>
						<equals>
							<property>gear/gear[1]/wow</property>
							<value>1</value>
						</equals>
						<equals>
							<property>gear/gear[2]/wow</property>
							<value>1</value>
						</equals>
					</or>
				</condition>
				<value>0</value>
			</u_min>
			<u_max>
				<condition>
					<and>
						<equals>
							<property>gear/gear[1]/wow</property>
							<value>0</value>
						</equals>
						<equals>
							<property>gear/gear[2]/wow</property>
							<value>0</value>
						</equals>
					</and>
				</condition>
				<value>30</value>
			</u_max>
			<u_max>
				<condition>
					<or>
						<equals>
							<property>gear/gear[1]/wow</property>
							<value>1</value>
						</equals>
						<equals>
							<property>gear/gear[2]/wow</property>
							<value>1</value>
						</equals>
					</or>
				</condition>
				<value>10</value>
			</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>PITCH DEG SYNC 2</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<or>
					<and>
						<not-equals>
							<property>/autopilot/locks/altitude</property>
							<value>vertical-speed-hold</value>
						</not-equals>
						<not-equals>
							<property>/autopilot/locks/altitude</property>
							<value>altitude-hold</value>
						</not-equals>
					</and>
					<not-equals>
						<property>/controls/flight/elevator-norm</property>
						<value>0</value>
					</not-equals>
				</or>
			</condition>
		</enable>
		<input>/orientation/pitch-deg</input>
		<output>/autopilot/internal/target-pitch-deg</output>
		<min>-30</min>
		<max>45</max>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: TARGET PITCH CMD</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<equals>
					<property>/autopilot/locks/altitude</property>
					<value>pitch-hold</value>
				</equals>
			</condition>
			<property>/orientation/pitch-deg</property>
		</input>
		<input>/autopilot/internal/target-pitch-deg</input>
		<output>/autopilot/internal/target-pitch</output>   	
	</filter>

	<filter>
		<name>System Command: Pitch Rate</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>-0.9</gain>
		<enable>
			<condition>
				<or>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>pitch-hold</value>
					</equals>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>vertical-speed-hold</value>
					</equals>
					<equals>
						<property>/autopilot/locks/altitude</property>
						<value>altitude-hold</value>
					</equals>
				</or>
			</condition>
		</enable>
		<input>
			<property>/orientation/pitch-deg</property>
		</input>
		<reference>
			<property>/autopilot/internal/target-pitch</property>
		</reference>
		<output>
			<property>/autopilot/internal/pitch-rate</property>
		</output>   	
		<min>-4.8</min>
		<max>4.8</max>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: PITCH RATE KP (P Gain)</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<table>
					<property>/velocities/mach</property>
					<entry><ind>0.2</ind><dep>-0.101</dep></entry>
					<entry><ind>1.0</ind><dep>-0.021</dep></entry>
					<entry><ind>2.1</ind><dep>-0.005</dep></entry>
				</table>
			</expression>
		</input>
		<output>/autopilot/internal/kp-pitch</output>
	</filter>
	
	<pid-controller>
		<name>IT-CONTROLLER: PITCH</name>
		<debug>false</debug>
		<enable>
			<condition>
				<and>
					<or>
						<equals>
							<property>/autopilot/locks/altitude</property>
							<value>pitch-hold</value>
						</equals>
						<equals>
							<property>/autopilot/locks/altitude</property>
							<value>vertical-speed-hold</value>
						</equals>
						<equals>
							<property>/autopilot/locks/altitude</property>
							<value>altitude-hold</value>
						</equals>
					</or>
					<equals>
						<property>/controls/flight/elevator-norm</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<property>/orientation/pitch-rate-degps</property>
		</input>
		<reference>
			<property>/autopilot/internal/pitch-rate</property>
		</reference>
		<output>
			<property>/fdm/jsbsim/systems/afcs/elevator-cmd-pid</property>
		</output>
		<config>
			<Kp>
				<property>/autopilot/internal/kp-pitch</property>
			</Kp>
			<beta>1</beta>
			<alpha>0.1</alpha>
			<gamma>0</gamma>
			<Ti>0.35</Ti>
			<Td>0.0002</Td>
			<u_min>-0.6</u_min>
			<u_max>0.6</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>IT-CONTROLLER: PITCH DEAD</name>
		<type>gain</type>
		<enable>
			<condition>
				<and>
					<not-equals>
						<property>/autopilot/locks/altitude</property>
						<value>pitch-hold</value>
					</not-equals>
                    <not-equals>
						<property>/autopilot/locks/altitude</property>
						<value>vertical-speed-hold</value>
					</not-equals>
					<not-equals>
						<property>/autopilot/locks/altitude</property>
						<value>altitude-hold</value>
					</not-equals>
				</and>
			</condition>
		</enable>
		<gain>1.0</gain>
		<input>0</input>
		<output>/fdm/jsbsim/systems/afcs/elevator-cmd-pid</output>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: ELEVATOR INVERT</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<and>
					<less-than-equals>
						<property>/orientation/roll-deg</property>
						<value>90</value>
					</less-than-equals>
					<greater-than-equals>
						<property>/orientation/roll-deg</property>
						<value>-90</value>
					</greater-than-equals>
				</and>
			</condition>
			<property>/fdm/jsbsim/systems/afcs/elevator-cmd-pid</property>
		</input>
		<input>
			<property>/fdm/jsbsim/systems/afcs/elevator-cmd-pid</property>
			<scale>-1.0</scale>
		</input>
		<output>/fdm/jsbsim/systems/afcs/elevator-cmd-delta</output>
	</filter>

